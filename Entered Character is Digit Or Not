//Entered Character is Digit Or Not
//To check whether the Entered character is a Digit or Not

#include<stdio.h>
int main()
{
    char ch;
    printf("Enter the character: ");
    scanf("%c",&ch);
    if(ch>='0' && ch<='9'){
      printf("Entered character is Digit : %c",ch);
    }
    else
    {
      printf("Entered character is Not Digit : %c",ch);
    }
    return 0;  
}


