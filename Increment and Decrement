int main(){
    int i=1;
    //post increment --first print and then increment
    printf("%d \n",i++);   //here first print the value of i   --1
    printf("%d \n",i);    //here it prints the incremented value of i++  --2
    
    //pre increment  --first increment and then print thr value
    printf("%d \n",++i); //--2 first increment the value and print 
    printf("%d \n",i);   //--2 print the value of i
    
}
